    <ProjectReference Include="..\D.Core.Application\D.Core.Application.csproj" />
    <ProjectReference Include="..\D.Core.Domain\D.Core.Domain.csproj" />
    <ProjectReference Include="..\D.Core.Infrastructure\D.Core.Infrastructure.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Template\bao_cao_doan_vao.docx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
